<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bm.hbm.InterfaceEntity" table="interface" schema="" catalog="brain">
        <id name="interfaceId">
            <column name="interface_id" sql-type="int" length="10" not-null="true"/>
        </id>
        <property name="interfaceChannelId">
            <column name="interface_channel_id" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="interfaceTypeId">
            <column name="interface_type_id" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="interfaceMobileNo">
            <column name="interface_mobile_no" sql-type="varchar" length="20"/>
        </property>
        <property name="interfaceImei">
            <column name="interface_imei" sql-type="varchar" length="25"/>
        </property>
        <property name="interfacePassword">
            <column name="interface_password" sql-type="varchar" length="128"/>
        </property>
        <property name="interfaceIp">
            <column name="interface_ip" sql-type="varchar" length="50"/>
        </property>
        <property name="interfaceMac">
            <column name="interface_mac" sql-type="varchar" length="50"/>
        </property>
        <property name="interfaceAutoSync">
            <column name="interface_auto_sync" sql-type="varchar" length="5"/>
        </property>
        <property name="interfaceSrv">
            <column name="interface_srv" sql-type="varchar" length="5"/>
        </property>
        <property name="interfaceActivation">
            <column name="interface_activation" sql-type="datetime" length="19"/>
        </property>
        <property name="interfaceBlock">
            <column name="interface_block" sql-type="datetime" length="19"/>
        </property>
        <property name="interfaceInactive">
            <column name="interface_inactive" sql-type="datetime" length="19"/>
        </property>
        <property name="interfaceCancel">
            <column name="interface_cancel" sql-type="datetime" length="19"/>
        </property>
        <property name="interfaceAttemptCtr">
            <column name="interface_attempt_ctr" sql-type="int" length="10"/>
        </property>
        <property name="interfaceStatusId">
            <column name="interface_status_id" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="interfaceCts">
            <column name="interface_cts" sql-type="timestamp" length="19" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>