<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bm.hbm.InterfaceTypeEntity" table="interface_type" schema="" catalog="brain">
        <id name="interfaceTypeId">
            <column name="interface_type_id" sql-type="int" length="10" not-null="true"/>
        </id>
        <property name="interfaceTypeCode">
            <column name="interface_type_code" sql-type="varchar" length="20" not-null="true"/>
        </property>
        <property name="interfaceTypeName">
            <column name="interface_type_name" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="interfaceTypeStatusId">
            <column name="interface_type_status_id" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="interfaceTypeCts">
            <column name="interface_type_cts" sql-type="timestamp" length="19" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>