<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bm.hbm.CountryEntity" table="country" schema="" catalog="brain">
        <id name="countryId">
            <column name="country_id" sql-type="int" length="10" not-null="true"/>
        </id>
        <property name="countryCode">
            <column name="country_code" sql-type="varchar" length="20" not-null="true"/>
        </property>
        <property name="countryName">
            <column name="country_name" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="countryStatusId">
            <column name="country_status_id" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="countryCts">
            <column name="country_cts" sql-type="timestamp" length="19" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>