<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bm.hbm.ServiceIntegratorEntity" table="service_integrator" schema="" catalog="brain">
        <id name="serviceIntegratorId">
            <column name="service_integrator_id" sql-type="int" length="10" not-null="true"/>
        </id>
        <property name="serviceIntegratorCode">
            <column name="service_integrator_code" sql-type="varchar" length="20" not-null="true"/>
        </property>
        <property name="serviceIntegratorName">
            <column name="service_integrator_name" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="serviceIntegratorCountryId">
            <column name="service_integrator_country_id" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="serviceIntegratorStatusId">
            <column name="service_integrator_status_id" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="serviceIntegratorCts">
            <column name="service_integrator_cts" sql-type="timestamp" length="19" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>