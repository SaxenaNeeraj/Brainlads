<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bm.hbm.PrefixEntity" table="prefix" schema="" catalog="brain">
        <id name="prefixId">
            <column name="prefix_id" sql-type="int" length="10" not-null="true"/>
        </id>
        <property name="prefixCode">
            <column name="prefix_code" sql-type="varchar" length="10" not-null="true"/>
        </property>
        <property name="prefixSiId">
            <column name="prefix_si_id" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="prefixStatusId">
            <column name="prefix_status_id" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="prefixCts">
            <column name="prefix_cts" sql-type="timestamp" length="19" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>